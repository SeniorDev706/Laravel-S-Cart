{
    "name": "s-cart/s-cart",
    "description": "Free Laravel e-commerce for business: shopping cart, cms content, and more...",
    "keywords": ["Laravel ecommerce","SCart ecommerce", "SCart open source", "Free Shopping cart", "Laravel multiple vendor", "Laravel marketplace", "Laravel multiple store",  "S-Cart"],
    "homepage": "https://s-cart.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Lanh Le",
            "email": "lanhktc@gmail.com"
        }
    ],
    "type" : "project",
    "require": {
        "php": "^8.0.2",
        "laravel/sanctum": "^3.0",
        "unisharp/laravel-filemanager": "^2.5",
        "spatie/laravel-pjax": "^2.2",
        "phpoffice/phpspreadsheet": "^1.20",
        "spatie/laravel-json-api-paginate": "^1.11",
        "spatie/db-dumper": "^3.0",
        "google/apiclient": "^2.0",
        "jenssegers/agent": "^2.6",
        "aws/aws-sdk-php": "^3.0",
        "barryvdh/laravel-dompdf": "^v1.0.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.0",
        "laravel/helpers": "^1.5",
        "laravel/legacy-factories": "^1.1",
        "laravel/socialite": "^5.13",
        "laravel/tinker": "^2.7",
        "laravel/ui": "~3.3"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1.0",
        "barryvdh/laravel-debugbar": "^3.6",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
